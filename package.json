  "packageManager": "pnpm@8.15.2",
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "scripts": {
    "extraScripts": "node scripts/syncGuides.js && node scripts/patches/defaultLanguage.js",
    "predev": "pnpm extraScripts",
    "dev": "astro dev --host",
