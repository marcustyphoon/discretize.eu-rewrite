    "@iconify-json/mdi": "^1.1.64",
    "@mdx-js/mdx": "^3.0.1",
    "astro": "4.5.16",
    "astro-icon": "^1.0.2",
    "astro-toc": "^0.1.3",
    "autoprefixer": "^10.4.16",
    "canvas-confetti": "^1.9.2",
    "classnames": "^2.5.1",
